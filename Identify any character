
#include<stdio.h>
int main()
{
	char chr;
	printf("Enter the character \n");
	scanf("%c", &chr);
	if(chr>='A'&& chr<='Z')
	{
		printf("%c is a capital letter \n",chr);
	}
	else if(chr>='a'&& chr<='b')
	{
		printf("%c is a small letter \n",chr);
	}
	else if(chr>='0'&& chr<='9')
	{
		printf("%c is a digit \n",chr);
	}
	else
	{
		printf("%c is a special symbol",chr);
	}
	return 0;
}
